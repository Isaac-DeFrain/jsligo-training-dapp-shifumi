export type t = 
    ["Stone"] | ["Paper"] | ["Scissor"];

const is_stone = (a:t) : bool => {
    match(a, {
        Stone: () => true,
        Paper: () => false,
        Scissor: () => false,
    })
};

const is_paper = (a:t) : bool => {
    match(a, {
        Stone: () => false,
        Paper: () => true,
        Scissor: () => false,
    })
};

const is_cisor = (a:t) : bool => {
    match(a, {
        Stone: () => false,
        Paper: () => false,
        Scissor: () => true,
    })
};
